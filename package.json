{
	"name": "templejs",
	"description": "A reactive Mustache view framework.",
	"author": "Beneath the Ink <info@beneaththeink.com>",
	"contributors": [
		{
			"name": "Tyler Johnson",
			"email": "tyler@beneaththeink.com",
			"url": "http://github.com/tyler-johnson"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/BeneathTheInk/temple.git"
	},
	"scripts": {
		"lint": "jshint src/ test/",
		"test": "npm run test-node",
		"test-node": "make && tape test/index.js",
		"test-browser": "make && browserify index.js --basedir test/ --debug | tape-run",
		"coverage-node": "make && istanbul cover test/index.js",
		"coverage-browser": "make && browserify index.js --basedir test/ -t browserify-istanbul --debug | node ./bin/browser-coverage.js && istanbul report --root coverage lcov",
		"report-coverage": "npm run coverage-browser && istanbul-coveralls --no-rm",
		"build": "make build-dist",
		"semantic-release": "semantic-release pre && npm run build && npm publish && semantic-release post"
	},
	"main": "./lib/temple.js",
	"jsnext:main": "./src/index.js",
	"superfast": "./lib/superfast.js",
	"dependencies": {
		"assign-props": "~1.1.2",
		"backbone-extend-standalone": "~0.1.2",
		"detect-indent": "~4.0.0",
		"dom-matches": "~2.0.0",
		"incremental-dom": "~0.3.0",
		"jsep": "~0.3.0",
		"lodash": "~4.x",
		"plain-merge": "~2.0.0",
		"raf": "~3.1.0",
		"source-map": "~0.5.3",
		"trackr": "~2.0.2",
		"trackr-objects": "~0.3.7"
	},
	"devDependencies": {
		"babel-preset-es2015-rollup": "~1.1.1",
		"browserify": "~13.0.0",
		"browserify-istanbul": "~0.2.1",
		"istanbul": "~0.4.2",
		"istanbul-coveralls": "~1.0.3",
		"jshint": "~2.9.1",
		"minimist": "~1.2.0",
		"pegjs": "~0.9.0",
		"rollup": "~0.25.3",
		"rollup-plugin-babel": "~2.3.9",
		"rollup-plugin-npm": "~1.4.0",
		"semantic-release": "^4.3.5",
		"tape": "~4.4.0",
		"tape-run": "~2.1.3",
		"uglify-js": "~2.6.1"
	},
	"license": "MIT",
	"keywords": [
		"view",
		"framework",
		"DOM",
		"html",
		"binding",
		"reactive",
		"dependency",
		"mustache"
	]
}
